@model IEnumerable<AISV2.Models.FileContactPerson>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ContactPerson)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.File)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContactPerson.ContactPersonID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.File.FileId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FileContactPersonID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FileContactPersonID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FileContactPersonID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
